//fixed length array
class Array = Object
{
    method newArray(size){
        native array_new
    }
    method new(size){
        this.data = this.newArray(size)
        this.size = size
        return this
    }
     method get(i){
        native array_get
    }
    method set(i,x){
        native array_set
    }
    method length(){
        return this.size
    }
}
class Vector = Object
{
    method new(){
        this.data = Array.new(64)
        this.capacity = 64
        this.size = 0
        return this
    }
    method get(idx){
        return this.data.get(idx)
    }
    method set(idx,x){
        this.data.set(idx,x)
    }
    method length(){
        return this.size
    }
    method append(x){
        if this.size + 1 < this.capacity {
            this.set(this.size,x)
            this.size = this.size + 1
        } else {
            arr = Array.new(this.capacity * 2)
            i = 0
            while i < this.size {
                arr.set(i,this.data.get(i)) //move the data
                i = i + 1
            }
            this.data = arr
            this.set(this.size,x)
            this.size = this.size + 1
        }
    }
}
class Main
{
    method main(){
       i = 0
       while i< 10000000 {
            i = i+1
       }
       System.print(i)
    }
}
